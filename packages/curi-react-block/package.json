{
  "name": "curi-react-block",
  "version": "0.1.0",
  "description": "A component prevents navigation",
  "main": "lib/index.js",
  "modules": "es",
  "scripts": {
    "test": "jest",
    "prebuild": "rimraf lib umd es",
    "build": "npm run build:lib && npm run build:umd && npm run build:min && npm run build:es",
    "build:es": "cross-env BABEL_ENV=es babel ./src -d es --ignore __tests__",
    "build:lib": "cross-env BABEL_ENV=cjs babel ./src -d lib --ignore __tests__",
    "build:umd": "cross-env BABEL_ENV=es rollup -c",
    "build:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -o umd/curi-react-block.min.js",
    "prepublish": "npm run build",
    "prettier": "prettier --single-quote --write \"{src,tests}/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+git://github.com/pshrmn/curi/tree/master/packages/curi-react-block"
  },
  "keywords": [
    "curi",
    "react",
    "block",
    "unload"
  ],
  "author": "Paul Sherman",
  "license": "MIT",
  "peerDependencies": {
    "react": "^15"
  },
  "dependencies": {
    "prop-types": "^15.5.8",
    "curi": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^4.0.0",
    "enzyme": "^2.8.2",
    "history": "^4.6.1",
    "jest": "^19.0.2",
    "prettier": "^1.2.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.2"
  }
}
