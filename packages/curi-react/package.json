    "curi-react-block": "^1.0.0-beta.2",
    "curi-react-curious": "^1.0.0-beta.2",
    "curi-react-link": "^1.0.0-beta.2",
    "curi-react-navigator": "^1.0.0-beta.2",
    "curi-react-redirect": "^1.0.0-beta.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "curi": "^1.0.0-beta.2",
    "in-publish": "^2.0.0",
    "prettier": "^1.5.2",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
