{
  "name": "curi-addon-prefetch",
  "version": "0.1.0",
  "description": "An addon to enable prefetching curi routes",
  "main": "lib/index.js",
  "files": [
    "lib",
    "umd",
    "es",
    "LICENSE",
    "*.md"
  ],
  "scripts": {
    "test": "jest",
    "prebuild": "rimraf lib umd es",
    "build": "npm run build:lib && npm run build:umd && npm run build:min && npm run build:es",
    "build:es": "cross-env BABEL_ENV=es babel ./src -d es --ignore __tests__",
    "build:lib": "cross-env BABEL_ENV=cjs babel ./src -d lib --ignore __tests__",
    "build:umd": "webpack --define process.env.NODE_ENV=\"'production'\" --output-filename=umd/curi-addon-prefetch.js",
    "build:min": "webpack -p --optimize-minimize --output-filename=umd/curi-addon-prefetch.min.js",
    "prepublish": "npm run build",
    "prettier": "prettier --single-quote --write \"{src,tests}/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/pshrmn/curi/tree/master/packages/curi-addon-prefetch"
  },
  "keywords": [
    "curi",
    "prefetch",
    "load",
    "addon"
  ],
  "author": "Paul Sherman",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^4.0.0",
    "jest": "^19.0.2",
    "prettier": "^1.2.2",
    "rimraf": "^2.6.1",
    "webpack": "^2.4.1"
  }
}
