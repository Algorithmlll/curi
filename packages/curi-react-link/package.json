{
  "name": "curi-react-link",
  "version": "0.1.0",
  "description": "A curi config aware React component that renders an <a>",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "prebuild": "rimraf lib umd es",
    "build": "npm run build:lib && npm run build:umd && npm run build:min && npm run build:es",
    "build:es": "cross-env BABEL_ENV=es babel ./src -d es --ignore __tests__",
    "build:lib": "cross-env BABEL_ENV=cjs babel ./src -d lib --ignore __tests__",
    "build:umd": "webpack --define process.env.NODE_ENV=\"'production'\" --output-filename=umd/curi-react-link.js",
    "build:min": "webpack -p --optimize-minimize --output-filename=umd/curi-react-link.min.js",
    "prepublish": "npm run build",
    "prettier": "prettier --single-quote --write \"{src,tests}/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+git://github.com/pshrmn/curi/tree/master/packages/curi-react-link"
  },
  "keywords": [
    "curi",
    "link",
    "navigation",
    "react",
    "anchor"
  ],
  "author": "Paul Sherman",
  "license": "MIT",
  "peerDependencies": {
    "react": "^15"
  },
  "dependencies": {
    "curi": "^0.6.0",
    "invariant": "^2.2.2",
    "prop-types": "^15.5.8"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^4.0.0",
    "enzyme": "^2.8.2",
    "history": "^4.6.1",
    "jest": "^19.0.2",
    "prettier": "^1.2.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "webpack": "^2.4.1"
  }
}
